<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.endproject">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity   android:name=".levelOne.levelOne"
                    android:screenOrientation="portrait">
        </activity>
        <activity   android:name=".NewPlayer"
            android:screenOrientation="portrait">
        </activity>
        <activity   android:name=".levelTwo.levelTwo"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".levelOne.levelOneLeaderboard"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".levelTwo.levelTwoLeaderboard"
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>